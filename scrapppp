
/***************************** [3.2] Understanding Properties*************************************** */
// with props, we can display dynamic data within a component
function Hello() {
  return (
    <div>
    <h1>Welcome to React!</h1>
    <p> Let's build something cool!</p>
    </div>
  )
}

const root6 = ReactDOM.createRoot(document.getElementById('root6'));

root6.render(
  <Hello />
)